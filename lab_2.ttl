@prefix : <http://example.org/vetclinic#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/vetclinic>
  rdf:type owl:Ontology ;
  rdfs:comment "Онтология ветеринарной клиники"@ru .


:Питомец
  rdf:type owl:Class ;
  rdfs:label "Питомец"@ru .

:Владелец
  rdf:type owl:Class ;
  rdfs:label "Владелец"@ru .

:Ветеринар
  rdf:type owl:Class ;
  rdfs:subClassOf :Сотрудник ;
  rdfs:label "Ветеринар"@ru .

:Сотрудник
  rdf:type owl:Class ;
  rdfs:label "Сотрудник"@ru .

:Приём
  rdf:type owl:Class ;
  rdfs:label "Приём"@ru .

:Диагноз
  rdf:type owl:Class ;
  rdfs:label "Диагноз"@ru .

:ВидЖивотного
  rdf:type owl:Class ;
  rdfs:label "Вид животного"@ru .


:имеетВладельца
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Питомец ;
  rdfs:range :Владелец ;
  rdfs:label "имеет владельца"@ru .

:владеетПитомцем
  rdf:type owl:ObjectProperty ;
  owl:inverseOf :имеетВладельца ;
  rdfs:label "владеет питомцем"@ru .

:видЖивотного
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Питомец ;
  rdfs:range :ВидЖивотного ;
  rdfs:label "вид животного"@ru .

:проводитПриём
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Ветеринар ;
  rdfs:range :Приём ;
  rdfs:label "проводит приём"@ru .

:приёмПитомца
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Приём ;
  rdfs:range :Питомец ;
  rdfs:label "приём питомца"@ru .

:поставленДиагноз
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Приём ;
  rdfs:range :Диагноз ;
  rdfs:label "поставлен диагноз"@ru .

:имеетПриём
  rdf:type owl:ObjectProperty ;
  owl:inverseOf :приёмПитомца ;
  rdfs:label "имеет приём"@ru .


:Питомец
  rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :имеетВладельца ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass :Владелец
  ] .

:Приём
  rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :приёмПитомца ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass :Питомец
  ] ;
  rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :поставленДиагноз ;
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass :Диагноз
  ] .