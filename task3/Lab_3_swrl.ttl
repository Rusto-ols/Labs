@prefix : <http://example.org/vetclinic#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .

<http://example.org/vetclinic_ru> a owl:Ontology ;
    rdfs:comment "Онтология ветеринарной клиники + SWRL-классификация."@ru .

:Животное a owl:Class .
:Кошка a owl:Class ; rdfs:subClassOf :Животное .
:Собака a owl:Class ; rdfs:subClassOf :Животное .
:Симптом a owl:Class .
:Диагноз a owl:Class .
:Отит a owl:Class ; rdfs:subClassOf :Диагноз .
:Лечение a owl:Class .
:Антибиотикотерапия a owl:Class ; rdfs:subClassOf :Лечение .

:ЖивотноеСОтитом a owl:Class ; rdfs:subClassOf :Животное .

:имеетСимптом a owl:ObjectProperty ; rdfs:domain :Животное ; rdfs:range :Симптом .
:имеетДиагноз a owl:ObjectProperty ; rdfs:domain :Животное ; rdfs:range :Диагноз .
:назначеноЛечение a owl:ObjectProperty ; rdfs:domain :Животное ; rdfs:range :Лечение .

:Барсик a :Кошка ;
    :имеетСимптом :ПокраснениеУшей , :ЧесоткаУшей .

:Шарик a :Собака ;
    :имеетСимптом :ПокраснениеУшей .

:ПокраснениеУшей a :Симптом .
:ЧесоткаУшей a :Симптом .

_:a a swrl:Variable ; swrl:varName "a" .

_:rule_otitis a swrl:Imp ;
  swrl:body (
    [ a swrl:ClassAtom ;
      swrl:classPredicate :Животное ;
      swrl:argument1 _:a ]
    [ a swrl:IndividualPropertyAtom ;
      swrl:propertyPredicate :имеетСимптом ;
      swrl:argument1 _:a ;
      swrl:argument2 :ПокраснениеУшей ]
    [ a swrl:IndividualPropertyAtom ;
      swrl:propertyPredicate :имеетСимптом ;
      swrl:argument1 _:a ;
      swrl:argument2 :ЧесоткаУшей ]
  ) ;
  swrl:head (
    [ a swrl:ClassAtom ;
      swrl:classPredicate :ЖивотноеСОтитом ;
      swrl:argument1 _:a ]
    [ a swrl:IndividualPropertyAtom ;
      swrl:propertyPredicate :имеетДиагноз ;
      swrl:argument1 _:a ;
      swrl:argument2 :Отит ]
  ) .
